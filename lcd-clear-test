lcdclear-test#include <LiquidCrystal.h>
LiquidCrystal lcd(1, 2, 4, 5, 6, 7);

void setup() {
  // put your setup code here, to run once:
  lcd.begin(16, 2);
}

void loop() {
  // put your main code here, to run repeatedly:

lcd.print("This is a test");
delay(3000);
lcd.clear();
delay(1000);
lcd.setCursor(2,1);
lcd.print("test 2");
delay(2000);
lcd.clear();
delay(1000);

}
